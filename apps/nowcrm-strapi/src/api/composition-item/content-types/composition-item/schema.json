{
  "kind": "collectionType",
  "collectionName": "composition_items",
  "info": {
    "singularName": "composition-item",
    "pluralName": "composition-items",
    "displayName": "CompositionItem"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "additional_prompt": {
      "type": "text"
    },
    "result": {
      "type": "text"
    },
    "composition": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::composition.composition",
      "inversedBy": "composition_items"
    },
    "attached_files": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "channel": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::channel.channel"
    },
    "item_status": {
      "type": "enumeration",
      "enum": [
        "Published",
        "Not published",
        "Scheduled",
        "None"
      ]
    }
  }
}
