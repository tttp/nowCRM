{
  "kind": "collectionType",
  "collectionName": "contact_documents",
  "info": {
    "singularName": "contact-document",
    "pluralName": "contact-documents",
    "displayName": "Contact-Document"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "text"
    },
    "file": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "contact": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::contact.contact",
      "inversedBy": "contact_documents"
    }
  }
}
